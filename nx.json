{
  "npmScope": "ngn-admin",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "projects": {
    "web": {
      "tags": ["web"]
    },
    "web-e2e": {
      "tags": ["web-e2e"],
      "implicitDependencies": [
        "web"
      ]
    },
    "auth": {
      "tags": ["api-auth"]
    },
    "api": {
      "tags": ["api-shared"]
    }
  }
}
